{
  "description": "Fetch transactions by date range on a single card",
  "endpoint": "GET /virtual-cards/:id/transactions",
  "headers": {
    "Authorization": "Bearer {Secret Key}",
    "Content-Type": "application/json"
  },
  "request_spec": {
    "type": ["params", "params_query"],
    "spec": [
      {
        "id": "Required: This is the unique id of the particular card you want to fetch all its transactions. You can get this id from the call to create a virtual card as data.id"
      },
      {
        "from": "Required: This is the start date of the transaction request period",
        "to": "Required: This is the end date of the transaction request period",
        "index": "Required: Pass 0 if you want to start from the beginning",
        "size": "Required: Specify how many transactions you want to retrieve in a single call"
      }
    ]
  },
