{
  "description": "Get a single transfer beneficiary details",
  "endpoint": "GET /beneficiaries/:id",
  "headers": {
    "Authorization": "Bearer {Secret Key}",
    "Content-Type": "application/json"
  },
  "request_spec": {
    "type": "params",
    "spec": {
      "id": "Required: This is the unique identifier for the beneficiary you intend to fetch. It is returned in the call to create a beneficiary as data.id"
    }
  },
  "sample_request": "GET /beneficiaries/2923",
  "sample_response": {
    "status": "success",
    "message": "Payout beneficiary fetched",
    "data": {
      "id": 2923,
      "account_number": "0690000032",
      "bank_code": "044",
      "full_name": "Pastor Bright",
      "meta": null,
      "created_at": "2019-11-28T08:15:29.000Z",
      "bank_name": "ACCESS BANK NIGERIA"
    }
  }
}
